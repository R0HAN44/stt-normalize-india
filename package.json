{
  "name": "spoken-number-normalizer",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "rdev": "npx tsx src/index.ts"
  },
  "keywords": [
    "speech-to-text",
    "normalization",
    "number-parser",
    "indian-numbers",
    "nlp",
    "streaming"
  ],
  "author": "Rohan J",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^25.0.8",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}